    "jsdom": "~22.1.0",
    "nx": "17.1.3",
    "nx-electron": "^16.1.0",
    "prettier": "^2.6.2",
    "react-scripts": "^5.0.1",
    "sass": "^1.86.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.0",
    "ts-jest-mock-import-meta": "^1.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^4.9.5",
    "vite": "~4.3.9",
    "vite-plugin-dts": "~2.3.0",
    "vitest": "~0.32.0",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.1.1",
    "webpack-merge": "^5.8.0"
  },
  "build": {
    "files": [
      "release/build",
      "node_modules",
      "node_modules/electron-debug",
      "node_modules/express",
      "package.json",
      ".config/buildResources/**/*",
      "board_api/"
    ],
    "directories": {
      "buildResources": "./.config/buildResources",
      "output": "release/out"
    },
    "extends": null,
    "publish": {
      "provider": "github",
      "owner": "offboard-studio",
      "repo": "renderer"
    },
    "productName": "Offboard Studio",
    "appId": "renderer.app",
    "asar": true,
    "asarUnpack": "**\\*.{node,dll}",
    "afterSign": ".config/scripts/notarize.js",
    "mac": {
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": ".config/entitlements.mac.plist",
      "entitlementsInherit": ".config/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    }
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "packageManager": "yarn@4.0.2"
}
